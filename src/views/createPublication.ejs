<%- include("partials/_header") %>

<div class="container d-flex justify-content-center">
    <div class="col-md-5">
      <div class="card">
        <div class="card-body">
          <form action="createPublication/new" method="post">
            <div class="mb-3">
              <label for="exampleInputEmail1" class="form-label">Nro de puerto</label>
              <input type="int" class="form-control" name="idPuerto">
            </div>
            <div class="mb-3">
              <label for="exampleInputEmail1" class="form-label">Descripcion</label>
              <input type="text" class="form-control" name="description">
            </div>
            <div class="mb-3">
              <label for="exampleInputPassword1" class="form-label">Matricula</label>
              <input type="text" class="form-control" name="plate">
            </div>
            <div class="mb-3">
              <label for="exampleInputPassword1" class="form-label">Manga</label>
              <input type="number" step="0.01" class="form-control" name="manga">
            </div>   
            <div class="mb-3">
              <label for="exampleInputPassword1" class="form-label">Eslora</label>
              <input type="number" step="0.01" class="form-control" name="eslora">
            </div> 
            <button type="submit" class="btn btn-primary">Submit</button>
          </form>
        </div>
      </div>    
    </div>
  </div>

<%- include("partials/_footer") %>